<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
   prefHeight="790.0" prefWidth="960.0" xmlns="http://javafx.com/javafx/"
   xmlns:fx="http://javafx.com/fxml/1"
   fx:controller="nz.ac.auckland.se206.controllers.ChatController">
   <children>
      <TextArea fx:id="chatTextArea" editable="false" layoutX="10.0" layoutY="130.0"
         prefHeight="477.0" prefWidth="938.0" wrapText="true">
         <font>
            <Font size="25.0" />
         </font>
      </TextArea>
      <TextField fx:id="inputText" layoutX="11.0" layoutY="629.0" prefHeight="55.0"
         prefWidth="693.0">
         <font>
            <Font size="20.0" />
         </font>
      </TextField>
      <Button fx:id="sendButton" layoutX="798.0" layoutY="607.0" mnemonicParsing="false"
         onAction="#onSendMessage" prefHeight="35.0" prefWidth="80.0" text="Write">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="goBackButton" layoutX="757.0" layoutY="655.0" mnemonicParsing="false"
         onAction="#onGoBack" prefHeight="43.0" prefWidth="164.0" text="Close the book">
